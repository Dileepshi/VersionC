package Ivyproject;

public class InfoonGIT {
	// $ git init - to create a repository
	// $ git remote add origin "url of git hub repository"
	
	// $ git status - for status 
	// $ git add -A  - add files to staging area
	// $ git commit -m "this is the commit" - to commit in the git repository
	// $ git push -u origin master - to move the file to github
	// $ git pull origin master - to get the files from github to git
	// $ git clone https://github.com/Dileepshi/EndtoEndProject.git - to get the code to the local system for the first time
	// $ git checkout -b branchname - to create a branch
	// $ git branch  -  to know on which branch u r in 
	
}
